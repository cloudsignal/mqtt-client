{
  "name": "@cloudsignal/mqtt-client",
"version": "2.2.1",
  "description": "CloudSignal WebSocket MQTT Client Library - Enterprise-grade MQTT client with V2 token management, AI agent communication patterns, and multi-transport support",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "browser": "dist/index.global.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./browser": "./dist/index.global.js"
  },
  "files": [
    "dist/",
    "README.md",
    "MIGRATION.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "mqtt",
    "websocket",
    "cloudsignal",
    "iot",
    "realtime",
    "messaging",
    "mqtt5",
    "ai-agent",
    "request-response",
    "typescript"
  ],
  "author": "CloudSignal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudsignal/mqtt-client.git"
  },
  "bugs": {
    "url": "https://github.com/cloudsignal/mqtt-client/issues"
  },
  "homepage": "https://cloudsignal.io",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "mqtt": "^5.14.1"
  }
}
